const url = "https://restcountries.com/v3.1/all"
const fetchdata = async() =>
{
    try 
    {
        const response = await fetch(url);
        const data = await response.json();
         let  ans = data.filter(obj => obj.region == "Asia").map(countries=>countries.name.common);
        console.log(ans)
    }
    catch(err)
    {
        console.log(err);
    }
    
}
fetchdata();


